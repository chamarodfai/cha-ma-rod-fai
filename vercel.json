{
  "name": "thai-tea-pos",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "BLOB_READ_WRITE_TOKEN": "@blob_read_write_token",
    "VERCEL_BLOB_STORE_URL": "https://mut17cdzoqscasrb.public.blob.vercel-storage.com"
  }
}